{
    "name": "emokykla/payment-statement-parser-bundle",
    "type": "symfony-bundle",
    "license": "UNLICENSED",
    "authors": [
        {
            "name": "Aurelijus Rožėnas",
            "email": "aurelijus.rozenas@gmail.com"
        },
        {
            "name": "Paulius Lukošiūnas",
            "email": "pauliuslukosiunas@gmail.com"
        },
        {
            "name": "Darius Ratkevičius",
            "email": "darius@ratkevicius.lt"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:emokykla/coding-standards.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/serializer": "4.4.*",
        "symfony/validator": "4.4.*"
    },
    "require-dev": {
        "emokykla/coding-standards": "^1.0",
        "kint-php/kint": "^3.3",
        "liip/functional-test-bundle": "^4.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.70",
        "phpstan/phpstan-symfony": "^0.12.16",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EMO\\PaymentStatementParserBundle\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "app/AppKernel.php",
            "app/kint_config.php"
        ],
        "psr-4": {
            "EMO\\PaymentStatementParserBundle\\Tests\\": "tests/"
        }
    }
}
